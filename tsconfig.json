{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "rootDir": "./src",
    "skipLibCheck": true,
    "outDir": "./dist",
    "jsx": "react",
    "module": "esnext",
    "jsxFactory": "h",
    "sourceMap": true,
    "baseUrl": "src",
    "paths": {
      "@store/*": ["store/*"],
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"]
    }
  }
}
